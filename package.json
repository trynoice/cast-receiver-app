{
  "name": "noice-cast-receiver-app",
  "description": "Chromecast custom receiver implementation for casting audio from Noice on Chromecast enabled devices",
  "repository": "git@github.com:trynoice/chromecast-receiver-app.git",
  "author": "Ashutosh Gangwar <ashutoshgngwr@gmail.com>",
  "license": "GPL-3.0-only",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn:watch-ts\" \"yarn:serve\"",
